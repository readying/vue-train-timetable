{
  "name": "vue-train-timetable",
  "version": "0.0.0",
  "description": "Vue Train Timetable Component",
  "repository": {
    "url": "Madao-3/vue-train-timetable",
    "type": "git"
  },
  "main": "dist/vue-train-timetable.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint . --ext .js",
    "test": "npm run lint && tyu",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "Madao",
    "email": "chrishyman256@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd"
    ],
    "name": "vue-train-timetable",
    "plugins": [
      "vue",
      "less"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended"
    ]
  },
  "devDependencies": {
    "bili": "^1.3.3",
    "eslint": "^4.14.0",
    "eslint-plugin-vue": "^4.0.1",
    "less-loader": "^4.1.0",
    "poi": "^9.3.10",
    "rollup-plugin-less": "^0.1.5",
    "rollup-plugin-vue": "2",
    "tyu": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.9"
  }
}
